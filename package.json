{
  "name": "nodejs-prompt",
  "version": "0.1.0",
  "description": "NodeJS prompt module",
  "main": "./dist/esm/index.js",
  "exports":{
    "default": "./dist/esm/index.js",
    "import": "./dist/esm/index.js",
    "require": "./dist/index.js"
  },
  "type": "module",
  "module": "./dist/esm/index.js",
  "scripts": {
    "test": "echo \"--NO TEST--\"",
    "tsc": "tsc",
    "build:common": "tsc --project tsconfig.json",
    "build:esm": "tsc --project tsconfig-esm.json",
    "build:clean": "rm -rf ./dist/*",
    "build": "npm run build:clean && npm run build:common && npm run build:esm",
    "exec": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simplyappdevs/nodejs-prompt.git"
  },
  "keywords": [
    "node",
    "prompt",
    "stdin",
    "readline",
    "module"
  ],
  "author": "SimplyAppDevs",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/simplyappdevs/nodejs-prompt/issues"
  },
  "homepage": "https://github.com/simplyappdevs/nodejs-prompt#readme",
  "devDependencies": {
    "@types/node": "^14.0.27",
    "typescript": "^3.9.7"
  }
}
